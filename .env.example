# Telegram Bot Token (required)
BOT_TOKEN=your_telegram_bot_token_here

# Development Bot Token (optional, used when NODE_ENV=development)
BOT_TOKEN_DEV=your_dev_bot_token_here

# Environment (development | production)
NODE_ENV=production

# Set to 'disabled' to disable the bot
BOT_STATUS=

# Request timeout in milliseconds (default: 15000)
TIMEOUT_MS=15000

# Admin Telegram chat ID for error notifications (optional)
ADMIN_CHAT_ID=
